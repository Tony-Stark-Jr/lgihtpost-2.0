{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:react/recommended", "airbnb", "prettier"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"react/react-in-jsx-scope": 0,
		"import/extensions": 0,
		"camelcase": 0
	},
	"settings": {
		"react": {
			"version": "18.2"
		}
	},
	"plugins": ["react-refresh"]
}
